      <% i = @game_run_log.size %>
      <% @game_run_log.each do |log| %>
        <%= "#{i} - #{log.created_at.to_s(:local)} - [#{log.log}]"%>
        <% i = i - 1 %>
        <br/>
      <% end %>
    </span>
  </body>
