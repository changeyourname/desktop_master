  <body>
    挂机列表
    <br/>
    <span style="color:red;font-size:14px;">
      <% @game_run_logs.each do |id| %>
        <a href="/detail?id=<%=id%>"><%=id%></a>
        &nbsp;&nbsp;|&nbsp;&nbsp;
      <% end %>
    </span>
    <hr>
    挂机详情 - <%= @game_run_log[0].no%> - <%= @game_run_log[0].user%>
